<?xml version="1.0" encoding="UTF-8"?><components xmlns:scr="http://www.osgi.org/xmlns/scr/v1.0.0">
    <scr:component name="org.aem.training.core.servlets.UserDataSelector">
        <implementation class="org.aem.training.core.servlets.UserDataSelector"/>
        <service servicefactory="false">
            <provide interface="javax.servlet.Servlet"/>
        </service>
        <property name="sling.servlet.resourceTypes">
            sling/servlet/default
        </property>
        <property name="sling.servlet.selectors">
            getdata
        </property>
        <property name="sling.servlet.extensions">
            html
        </property>
        <property name="sling.servlet.methods">
            GET
        </property>
        <property name="service.vendor" value="Adobe"/>
        <property name="service.pid" value="org.aem.training.core.servlets.UserDataSelector"/>
    </scr:component>
</components>
